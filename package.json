    "grunt-contrib-jshint": "^1.0.0",
    "grunt-jsbeautifier": "^0.2.13",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.0",
    "zuul": "^3.3.0"
  },
  "dependencies": {
    "@nathanfaucett/environment": "^0.0.1",
    "@nathanfaucett/mixin": "0.0.1",
    "@nathanfaucett/template": "^0.0.1",
